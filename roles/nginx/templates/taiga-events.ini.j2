[watcher:taiga-events]
working_dir = /home/{{ taiga_events_username }}/taiga-events
cmd = /home/{{ taiga_events_username }}/taiga-events/node_modules/coffee-script/bin/coffee
args = index.coffee
uid = taiga
numprocesses = 1
autostart = true
send_hup = true
stdout_stream.class = FileStream
stdout_stream.filename = /home/{{ taiga_events_username }}/logs/taigaevents.stdout.log
stdout_stream.max_bytes = 10485760
stdout_stream.backup_count = 12
stderr_stream.class = FileStream
stderr_stream.filename = /home/{{ taiga_events_username }}/logs/taigaevents.stderr.log
stderr_stream.max_bytes = 10485760
stderr_stream.backup_count = 12
